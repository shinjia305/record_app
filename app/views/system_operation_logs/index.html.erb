<div>
  <h1 class="font-bold text-4xl">ログ操作確認画面</h1>
  <table>
    <tr>
      <th>管理者id</th>
      <th>操作内容</th>
    </tr>
    <% @system_operation_logs.each do |system_operation_log| %>
      <tr>
        <td><%= system_operation_log.user_id %></td>
        <td><%=system_operation_log.message %></td>
      </tr>
    <% end %>
  </table>
</div>
